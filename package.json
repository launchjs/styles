{
  "name": "@launch/styles",
  "version": "0.1.0",
  "description": "Launch.js - CSS/SASS/Less/PostCSS preset",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .build",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchjs/styles.git"
  },
  "keywords": [
    "launch.js",
    "css",
    "sass",
    "postcss",
    "preset"
  ],
  "author": "Lee Benson <lee@leebenson.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/launchjs/styles/issues"
  },
  "homepage": "https://github.com/launchjs/styles#readme",
  "devDependencies": {
    "@launch/app": "^0.15.0",
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.3",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7"
  },
  "peerDependencies": {
    "@launch/app": ">= 0.15.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "__tests__/.+\\.(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
