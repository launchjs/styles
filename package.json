{
  "name": "@launch/styles",
  "version": "1.1.0",
  "description": "Launch.js - CSS/Sass/Less/PostCSS preset",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .build",
    "dev": "ts-node __tests__/dev.e2e.ts",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm run build",
    "prod": "ts-node __tests__/prod.e2e.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/launchjs/styles.git"
  },
  "keywords": [
    "launch.js",
    "css",
    "sass",
    "postcss",
    "preset"
  ],
  "author": "Lee Benson <lee@leebenson.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/launchjs/styles/issues"
  },
  "homepage": "https://github.com/launchjs/styles#readme",
  "devDependencies": {
    "@launch/app": "^0.25.0",
    "@launch/entry": "^1.4.0",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "jest": "^22.4.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/mini-css-extract-plugin": "^0.2.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.4",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0"
  },
  "peerDependencies": {
    "@launch/app": ">= 0.15.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "__tests__/.+\\.(test|spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  }
}
